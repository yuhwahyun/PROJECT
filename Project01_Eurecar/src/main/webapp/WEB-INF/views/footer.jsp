<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
	<style type="text/css">
		footer{
			margin:0 auto;
			width:1280px;
			padding-top: 20px;
		}
		
		.logo{
			float: left;
		}
		
		.logo img{
			width:110px;
			height:20px; 
		}
		.logo p{
			color: #fff;
			padding-bottom: 20px;
		}
		
		.icon{
			float: right;
		}
		
		.icon ul li{
			list-style: none;
			float: left;
			padding-left:15px;
		}

		.icon ul li a img{
			width:45px;
			height: 45px;
		}
		
		
		
	</style>
</head>

<body>
	<footer>
			<div class="logo">
				<img src="./image/logo.png">
				<p>Copyright ⓒ Team C All Rights Reserved</p>
			</div>
			
			<div class="icon">
				<ul>
					<li><a href="#"><img src="./image/facebook.png" alt="페이스북"></a></li>
					<li><a href="#"><img src="./image/insta.png" alt="인스타"></a></li>
				</ul>
			</div>
	</footer>
</body>
</html>